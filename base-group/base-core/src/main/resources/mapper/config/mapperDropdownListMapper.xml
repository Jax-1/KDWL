<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.base.dao.config.DropdownListMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.app.base.dto.config.DropdownList">
    <result column="id" property="id" />
    <result column="date_entered" property="dateEntered" />
    <result column="created_by" property="createdBy" />
        <result column="type_code" property="typeCode" />
        <result column="item_code" property="itemCode" />
        <result column="item_name" property="itemName" />
        <result column="item_remarks" property="itemRemarks" />
        <result column="parent_code" property="parentCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
id,
date_entered AS dateEntered,
created_by AS createdBy,
        type_code AS typeCode, item_code AS itemCode, item_name AS itemName, item_remarks AS itemRemarks, parent_code AS parentCode
    </sql>

</mapper>
