<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.base.dao.config.CodeTypeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.app.base.dto.config.CodeType">
        <id column="type_code" property="typeCode" />
    <result column="date_entered" property="dateEntered" />
        <result column="type_name" property="typeName" />
        <result column="create_by" property="createBy" />
        <result column="remarks" property="remarks" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
date_entered AS dateEntered,
        type_code AS typeCode, type_name AS typeName, create_by AS createBy, remarks
    </sql>

</mapper>
